<!-- NullpoMino Config Tool -->
<frame resourcebundle="cx.it.nullpo.nm8.tool.configtool.lang.configtool" title="@title" defaultcloseoperation="EXIT_ON_CLOSE">
	<borderlayout>
		<constraint location="Center">
			<tabbedpane>
				<!-- System Settings -->
				<tab title="@tab_system">
					<panel>
						<borderlayout>
							<constraint location="North">
								<panel>
									<gridlayout rows="0" columns="1">
										<!-- Screen Resolution -->
										<panel>
											<borderlayout>
												<constraint location="West">
													<label text="@sys_combobox_resolution_label" />
												</constraint>
												<constraint location="Center">
													<combobox id="sys_combobox_resolution" tooltiptext="@sys_combobox_resolution_tooltip">
														<defaultcomboboxmodel setas="model">
															<string text="320x240"/>
															<string text="400x300"/>
															<string text="480x360"/>
															<string text="512x384"/>
															<string text="640x480"/>
															<string text="800x600"/>
															<string text="1024x768"/>
															<string text="1152x864"/>
															<string text="1280x960"/>
														</defaultcomboboxmodel>
													</combobox>
												</constraint>
											</borderlayout>
										</panel>

										<!-- Sound Provider -->
										<panel>
											<borderlayout>
												<constraint location="West">
													<label text="@sys_combobox_soundprovider_label" />
												</constraint>
												<constraint location="Center">
													<combobox id="sys_combobox_soundprovider" tooltiptext="@sys_combobox_soundprovider_tooltip">
														<defaultcomboboxmodel setas="model">
															<string text="Java Sound"/>
															<string text="OpenAL (LWJGL)"/>
															<string text="OpenAL (JOAL)"/>
														</defaultcomboboxmodel>
													</combobox>
												</constraint>
											</borderlayout>
										</panel>

										<!-- Sound volume -->
										<panel>
											<borderlayout>
												<constraint location="West">
													<label text="@sys_txtfld_volume_label" />
												</constraint>
												<constraint location="Center">
													<textfield id="sys_txtfld_volume" tooltiptext="@sys_txtfld_volume_tooltip" />
												</constraint>
											</borderlayout>
										</panel>

										<!-- FPS -->
										<panel>
											<borderlayout>
												<constraint location="West">
													<!-- JSpinner doesn't seem to support tooltip, so... -->
													<label text="@sys_spinner_fps_label" tooltiptext="@sys_spinner_fps_tooltip" />
												</constraint>
												<constraint location="Center">
													<spinner id="sys_spinner_fps">
														<spinnernumbermodel setas="model">
															<int value="0" setas="minimum" />
															<int value="300" setas="maximum" />
															<int value="1" setas="stepsize" />
															<int value="60" setas="value" />
														</spinnernumbermodel>
													</spinner>
												</constraint>
											</borderlayout>
										</panel>

										<!-- Checkboxes -->
										<checkbox text="@sys_chkbox_fullscreen_label"
										          id="sys_chkbox_fullscreen"
										          tooltiptext="@sys_chkbox_fullscreen_tooltip" />
										<checkbox text="@sys_chkbox_enablesound_label"
										          id="sys_chkbox_enablesound"
										          tooltiptext="@sys_chkbox_enablesound_tooltip" />
										<checkbox text="@sys_chkbox_nativelookfeel_label"
										          id="sys_chkbox_nativelookfeel"
										          tooltiptext="@sys_chkbox_nativelookfeel_tooltip" />
										<checkbox text="@sys_chkbox_enablealpha_label"
										          id="sys_chkbox_enablealpha"
										          tooltiptext="@sys_chkbox_enablealpha_tooltip" />
										<checkbox text="@sys_chkbox_enablecolorfilter_label"
										          id="sys_chkbox_enablecolorfilter"
										          tooltiptext="@sys_chkbox_enablecolorfilter_tooltip" />
										<checkbox text="@sys_chkbox_enablegradient_label"
										          id="sys_chkbox_enablegradient"
										          tooltiptext="@sys_chkbox_enablegradient_tooltip" />
									</gridlayout>
								</panel>
							</constraint>
						</borderlayout>
					</panel>
				</tab>

				<!-- Swing Settings -->
				<tab title="@tab_swing">
					<panel>
